[{
  "selector": "core",
  "style": {
    "selection-box-color": "#AAD8FF",
    "selection-box-border-color": "#8BB0D0",
    "selection-box-opacity": "0.5"
  }
}, {
  "selector": "node",
  "style": {
    "width": "mapData(score, 460, 101246, 80, 296)",
    "height": "mapData(score, 460, 101246, 80, 296)",
    "font-size": "50px",
    "text-valign": "center",
    "text-halign": "center",
    "background-color": "#555",
    "text-outline-color": "#555",
    "text-outline-width": "10px",
    "color": "#fff",
    "overlay-padding": "6px",
    "z-index": "10"
  }
}, {
    "selector": "node[score > 8000]",
    "style": {
      "content": "data(name)"
    }
}, {
    "selector": "node[idInt = 0]",
    "style": {
      "font-size": "60px",
      "font-weight": "bold"
    }
}, {
  "selector": "node:selected",
  "style": {
    "content": "data(name)",
    "border-width": "6px",
    "border-color": "#AAD8FF",
    "border-opacity": "0.5",
    "background-color": "#77828C",
    "text-outline-color": "#77828C"
  }
}, {
  "selector": "edge",
  "style": {
    "curve-style": "haystack",
    "haystack-radius": "0.5",
    "opacity": "0.5",
    "line-color": "mapData(weight, 263, 9000, #bbb , black)",
    "width": "mapData(weight, 263, 13624, 4, 20)",
    "overlay-padding": "3px"
  }
}, {
  "selector": "node.unhighlighted",
  "style": {
    "opacity": "0.2"
  }
}, {
  "selector": "edge.unhighlighted",
  "style": {
    "opacity": "0.05"
  }
}, {
  "selector": ".highlighted",
  "style": {
    "z-index": "999999"
  }
}, {
  "selector": "node.highlighted",
  "style": {
    "border-width": "2px",
    "border-color": "#AAD8FF",
    "border-opacity": "1",
    "background-color": "#394855",
    "text-outline-color": "#394855"
  }
}]
